	doi = {10.1016/j.enbuild.2013.11.066},
	abstract = {Thermal comfort is one of the main driving factors in defining the operational settings of {HVAC} systems, and it greatly impacts energy efficiency in buildings. Lack of information about human related variables results in using unrepresentative operational settings, which in turn could bring about low efficiency in {HVAC} operations. In this paper, the implementation and evaluation of a framework that integrates building occupants’ personalized thermal profiles into the {HVAC} control logic is presented. The framework enables occupants to communicate their preferences for indoor thermal conditions through a user interface, leveraging a participatory sensing approach. The framework learns occupants’ comfort profiles, using a fuzzy predictive model, and controls the {HVAC} system using a complementary control strategy, which enables the framework to be implemented in existing centrally controlled {HVAC} systems with minimum intrusion. Evaluation of the framework in a real building setting showed user comfort improvement. Moreover, the results showed a 39\% reduction in daily average airflow when the {HVAC} system conditions the rooms at occupants’ desired temperatures. Airflow is proportional to the energy consumption of {HVAC} system components. Consequently, the implementation of the framework shows improvements in the efficiency of the {HVAC} system's performance for centrally controlled office buildings.},
	pages = {398--410},
	journaltitle = {Energy and Buildings},
	shortjournal = {Energy and Buildings},
	author = {Jazizadeh, Farrokh and Ghahramani, Ali and Becerik-Gerber, Burcin and Kichkaylo, Tatiana and Orosz, Michael},
	urldate = {2019-09-22},
	date = {2014-02-01},
	keywords = {Thermal comfort, Energy efficiency, {HVAC} control, Participatory sensing, Personalized control},
	file = {ScienceDirect Full Text PDF:/Users/sadaharu/Zotero/storage/DM3IUZ7H/Jazizadeh et al. - 2014 - User-led decentralized thermal comfort driven HVAC.pdf:application/pdf;ScienceDirect Snapshot:/Users/sadaharu/Zotero/storage/P3AJNYXL/S0378778813007731.html:text/html}
}

@article{nagy_occupant_2016,
	title = {Occupant centered lighting control: A user study on balancing comfort, acceptance, and energy consumption},
	volume = {126},
	issn = {0378-7788},
	url = {http://www.sciencedirect.com/science/article/pii/S0378778816304534},
	doi = {10.1016/j.enbuild.2016.05.075},
	shorttitle = {Occupant centered lighting control},
	abstract = {The energy savings of buildings promised by automatic control systems are often ineffective due to occupants who may not use the features of the system to the full potential at best, or deactivate the system at worst. Therefore, systems that adapt to the occupant over time without compromising his/her comfort are needed. We have previously introduced the occupant centered lighting control ({OCC}) approach, where the set-points are automatically derived through interaction with the occupant, and demonstrated the energy savings potential. In this work, we present a user study on {OCC} in a 12-week experiment under real-world conditions in six offices with ten participants. We find generally good user satisfaction, except for those users who deem automatic lighting conrol unimportant. The Control On action is virtually never overridden, while the Control Off action is accepted in over 75\% of the cases. Finally, we determine an average 13.4\% decrease in the normalized energy consumption without significant impact on the comfort on the occupants.},
	pages = {310--322},
	journaltitle = {Energy and Buildings},
	shortjournal = {Energy and Buildings},
	author = {Nagy, Zoltán and Yong, Fah Yik and Schlueter, Arno},
	urldate = {2019-09-22},
	date = {2016-08-15},
	keywords = {Energy efficient buildings, Building control, Occupant centered control, Smart buildings, User interaction},
	file = {ScienceDirect Full Text PDF:/Users/sadaharu/Zotero/storage/DL4JYL3Y/Nagy et al. - 2016 - Occupant centered lighting control A user study o.pdf:application/pdf;ScienceDirect Snapshot:/Users/sadaharu/Zotero/storage/ZLTQG4F6/S0378778816304534.html:text/html}
}

@article{nagy_occupant_2015,
	title = {Occupant centered lighting control for comfort and energy efficient building operation},
	volume = {94},
