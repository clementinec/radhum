}

@article{wang_observations_2007,
	title = {Observations of upper-extremity skin temperature and corresponding overall-body thermal sensations and comfort},
	volume = {42},
	issn = {0360-1323},
	url = {http://www.sciencedirect.com/science/article/pii/S0360132306003647},
	doi = {10.1016/j.buildenv.2006.06.035},
	series = {Indoor Air 2005 Conference},
	abstract = {This paper explores how upper extremity skin temperatures correlate with overall-body thermal sensation. Skin temperature measurements of the finger, hand, and forearm might be useful in monitoring and predicting people's thermal state. Subjective perceptions of overall thermal sensation and comfort were collected by repeated surveys, for subjects in a range of test chamber temperatures. A positive temperature gradient (finger warmer than the forearm) of as much as 2K was seen when subjects felt warm and hot, while a negative temperature gradient (finger colder than the forearm) as much as 8.5K was seen for cool and cold subjects. A useful warm/cold boundary of 30°C was found in finger temperature, for both steady state and transient conditions. When finger temperature was above 30°C, or finger-forearm skin temperature gradient above 0K, there was no cool discomfort. When finger temperature was below 30°C, or the finger-forearm skin temperature gradient less than 0K, cool discomfort was a possibility. Finger temperature and finger-forearm temperature gradient are very similar in their correlation to overall sensation. We also examine how overall sensation is affected by actively manipulating the hand's temperature.},
	pages = {3933--3943},
	number = {12},
	journaltitle = {Building and Environment},
	shortjournal = {Building and Environment},
	author = {Wang, Danni and Zhang, Hui and Arens, Edward and Huizenga, Charlie},
	urldate = {2019-08-30},
	date = {2007-12-01},
	keywords = {Thermal comfort, Skin temperature, Thermal sensation, Comfort threshold, Finger, Forearm, Hand, Occupant survey, Temperature gradient},
	file = {ScienceDirect Full Text PDF:/Users/sadaharu/Zotero/storage/A4Q6BHHI/Wang et al. - 2007 - Observations of upper-extremity skin temperature a.pdf:application/pdf;ScienceDirect Snapshot:/Users/sadaharu/Zotero/storage/KKDFPK7F/S0360132306003647.html:text/html}
}

@inproceedings{auffenberg_personalised_2015,
	title = {A Personalised Thermal Comfort Model Using a Bayesian Network},
	isbn = {978-1-57735-738-4},
	url = {http://dl.acm.org/citation.cfm?id=2832581.2832605},
	series = {{IJCAI}'15},
	abstract = {In this paper, we address the challenge of predicting optimal comfort temperatures of individual users of a smart heating system. At present, such systems use simple models of user comfort when deciding on a set point temperature. These models generally fail to adapt to an individual user's preferences, resulting in poor estimates of a user's preferred temperature. To address this issue, we propose a personalised thermal comfort model that uses a Bayesian network to learn and adapt to a user's individual preferences. Through an empirical evaluation based on the {ASHRAE} {RP}-884 data set, we show that our model is consistently 17.5- 23.5\% more accurate than current models, regardless of environmental conditions and the type of heating system used. Our model is not limited to a single metric but can also infer information about expected user feedback, optimal comfort temperature and thermal sensitivity at the same time, which can be used to reduce energy used for heating with minimal comfort loss.},
	pages = {2547--2553},
	booktitle = {Proceedings of the 24th International Conference on Artificial Intelligence},
	publisher = {{AAAI} Press},
	author = {Auffenberg, Frederik and Stein, Sebastian and Rogers, Alex},
	urldate = {2019-08-30},
	date = {2015},
	note = {event-place: Buenos Aires, Argentina}
}

@article{salamone_integrated_2018,
	title = {Integrated Method for Personal Thermal Comfort Assessment and Optimization through Users’ Feedback, {IoT} and Machine Learning: A Case Study †},
	volume = {18},
	rights = {http://creativecommons.org/licenses/by/3.0/},
