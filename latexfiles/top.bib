	shortjournal = {Weather and Climate Extremes},
	author = {Zare, Sajad and Hasheminejad, Naser and Shirvan, Hossein Elahi and Hemmatjo, Rasoul and Sarebanzadeh, Keyvan and Ahmadi, Saeid},
