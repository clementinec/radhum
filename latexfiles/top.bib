	langid = {english},
	keywords = {Skin temperature, Non-steady state, Regression, Time differential, Whole-body thermal sensation},
	file = {ScienceDirect Snapshot:/Users/sadaharu/Zotero/storage/YGQ49VB3/S0360132313001820.html:text/html}
}

@article{bermejo_design_2012,
	title = {Design and simulation of a thermal comfort adaptive system based on fuzzy logic and on-line learning},
	volume = {49},
	issn = {0378-7788},
	url = {http://www.sciencedirect.com/science/article/pii/S0378778812001247},
	doi = {10.1016/j.enbuild.2012.02.032},
	abstract = {Indoor thermal comfort is the most commonly studied type of comfort in the literature. We can find works which try to predict the user's satisfaction or keep static conditions by means of black-box controllers. However, we propose a novel system which is capable of adapting to the user's thermal preferences without any prior knowledge, and measuring his comfort level by aggregating several thermal parameters into one single thermal index. This single value is used in a static set of fuzzy rules easily understood by the user, and the labels used in those rules are dynamically adapted to the estimated preferences of the user. Experimental simulation shows that our proposal is capable of learning on-line the optimal thermal feeling for the user, and anticipating the necessary actions to obtain such thermal comfort in an indoor environment. The ubiquitous nature of nodes in a Wireless Sensor Networks ({WSN}) opens up wide possibilities for combining its distributed sensing power with advanced adaptive real-time learning systems. Thus, this paper also presents a proposal of integration of a thermal comfort adaptive fuzzy system into Coral2K®, a centralized platform for monitoring and control heterogeneous {WSN} networks.},
	pages = {367--379},
	journaltitle = {Energy and Buildings},
	shortjournal = {Energy and Buildings},
	author = {Bermejo, Pablo and Redondo, Luis and de la Ossa, Luis and Rodríguez, Daniel and Flores, Julia and Urea, Carmen and Gámez, José A. and Puerta, José M.},
	urldate = {2019-10-22},
	date = {2012-06-01},
	langid = {english},
	keywords = {Energy, Comfort, Efficiency, Adaptive, Coral2K, Fuzzy Logic, {WSN}},
	file = {ScienceDirect Full Text PDF:/Users/sadaharu/Zotero/storage/3VZVNGRE/Bermejo et al. - 2012 - Design and simulation of a thermal comfort adaptiv.pdf:application/pdf;ScienceDirect Snapshot:/Users/sadaharu/Zotero/storage/2S54TWXX/S0378778812001247.html:text/html}
