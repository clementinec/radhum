	author = {Zeinelabdein, Rami and Omer, Siddig and Gan, Guohui},
	urldate = {2019-09-17},
	date = {2018-02-01},
	keywords = {Free cooling, Building cooling, Latent heat storage ({LHS}), Phase change material ({PCM}), Thermal energy storage ({TES})},
	file = {ScienceDirect Full Text PDF:/Users/sadaharu/Zotero/storage/36E5ECDS/Zeinelabdein et al. - 2018 - Critical review of latent heat storage systems for.pdf:application/pdf;ScienceDirect Snapshot:/Users/sadaharu/Zotero/storage/MU6IVDAB/S1364032117314223.html:text/html}
}

