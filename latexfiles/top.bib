@article{hoppe_different_2002,
	title = {Different aspects of assessing indoor and outdoor thermal comfort},
	volume = {34},
	issn = {0378-7788},
	url = {http://www.sciencedirect.com/science/article/pii/S0378778802000178},
	doi = {10.1016/S0378-7788(02)00017-8},
	series = {Special Issue on Thermal Comfort Standards},
	abstract = {As people spend most of their time indoors, steady state models may be appropriate for thermal comfort assessments there, while, for the relatively short times spent outdoors (mostly less than 1h), those models tend to overestimate discomfort. This discrepancy is larger for cool outdoor conditions than for warm ones. For example, for a person leaving a room in thermal comfort (Tsk=33.5°C, Tcore=37 °C) into cold winter outdoor conditions (Ta=Tmrt=0°C, {VP}=5hPa, v=1m/s) obtaining steady state will take many hours, while leaving into hot conditions (Ta=30°C, Tmrt=60°C, {VP}=15hPa, v=0.5m/s) it will be reached within less than 30min. Consequently, especially for outdoor thermal comfort assessments in cold conditions, non-steady state models should be applied. This, among other new aspects, will be considered in a new internationally standardised Universal Thermal Climate Index ({UTCI}), which is currently being developed by a working group of the International Society of Biometeorology. Besides such physical/physiological approaches, psychological factors also have to be considered, i.e. diverging thermal expectations indoors and outdoors. Consequently, different approaches are necessary for assessing indoor or outdoor thermal comfort.},
	pages = {661--665},
	number = {6},
	journaltitle = {Energy and Buildings},
	shortjournal = {Energy and Buildings},
	author = {Höppe, Peter},
	urldate = {2019-08-30},
	date = {2002-07-01},
	keywords = {Thermal comfort, Indoor comfort, Outdoor comfort},
	file = {ScienceDirect Full Text PDF:/Users/sadaharu/Zotero/storage/98BX4BYF/Höppe - 2002 - Different aspects of assessing indoor and outdoor .pdf:application/pdf;ScienceDirect Snapshot:/Users/sadaharu/Zotero/storage/YTM4S4NH/S0378778802000178.html:text/html}
}

@article{hoppe_aspects_1997,
