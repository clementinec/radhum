	keywords = {Heat, Temperature, {UTCI}, Heat indices, Thermal climate},
	file = {ScienceDirect Full Text PDF:/Users/sadaharu/Zotero/storage/A5JPDGRG/Zare et al. - 2018 - Comparing Universal Thermal Climate Index (UTCI) w.pdf:application/pdf;ScienceDirect Snapshot:/Users/sadaharu/Zotero/storage/R4SKRJG2/S221209471730110X.html:text/html}
