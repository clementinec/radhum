@article{henriques_exergy_2016,
	title = {Exergy model of the human heart},
	volume = {117},
	issn = {0360-5442},
	url = {http://www.sciencedirect.com/science/article/pii/S0360544216302122},
	doi = {10.1016/j.energy.2016.02.150},
	series = {The 28th International Conference on Efficiency, Cost, Optimization, Simulation and Environmental Impact of Energy Systems - {ECOS} 2015},
	abstract = {A precedent study about the effects of obesity on exergy behavior concluded that higher body fat per se does not lead to higher mortality among obese people; the focus must be turned to obesity-related diseases. Therefore, the next step towards the understanding of the effects of pathologies on the exergy behavior of human body is to analyze human heart. In the present work, an exergy model of human heart is developed. The heart is divided into two control volumes: the left heart, which pumps arterial blood from the lungs to the organs, and the right heart, which pumps venous blood from the organs to the lungs. Exergy of metabolism, exergy transfer associated to heat of metabolism, difference of exergy of blood flows and performed work are taken into account in the exergy balance for normotensive and hypertensive people under different levels of exercise. The left part of the heart is always responsible for more than 80\% of the total exergy destruction. Moreover, a hypertensive heart destroys more exergy; this surplus value, when integrated along life cycle, represents 170 {MJ}/kg of destroyed exergy, which, according to the rate of living theory, would lead to a loss of 4.4 years in life expectancy.},
	pages = {612--619},
	issue = {Part 2},
	journaltitle = {Energy},
	shortjournal = {Energy},
	author = {Henriques, Izabela Batista and Mady, Carlos Eduardo Keutenedjian and de Oliveira Junior, Silvio},
	date = {2016-12-15},
	keywords = {Exergy Analysis, Destroyed exergy, Exergy efficiency, Human heart},
	file = {Energy Volume issue 2016 [doi 10.1016%2Fj.energy.2016.02.150] Henriques, Izabela Batista\; Mady, Carlos Eduardo Keutenedjian\; d -- Exergy model of the human heart.pdf:/Users/sadaharu/Zotero/storage/J2KIIPZ7/Energy Volume issue 2016 [doi 10.1016%2Fj.energy.2016.02.150] Henriques, Izabela Batista\; Mady, Carlos Eduardo Keutenedjian\; d -- Exergy model of.pdf:application/pdf;ScienceDirect Snapshot:/Users/sadaharu/Zotero/storage/FX64D7WS/S0360544216302122.html:text/html}
