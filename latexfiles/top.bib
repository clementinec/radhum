@article{lindberg_solweig_2008,
	title = {{SOLWEIG} 1.0 – Modelling spatial variations of 3D radiant fluxes and mean radiant temperature in complex urban settings},
	volume = {52},
	issn = {0020-7128, 1432-1254},
	url = {https://link.springer.com/article/10.1007/s00484-008-0162-7},
	doi = {10.1007/s00484-008-0162-7},
	abstract = {The mean radiant temperature, Tmrt, which sums up all shortwave and longwave radiation fluxes (both direct and reflected) to which the human body is exposed is one of the key meteorological parameters governing human energy balance and the thermal comfort of man. In this paper, a new radiation model ({SOLWEIG} 1.0), which simulates spatial variations of 3D radiation fluxes and Tmrt in complex urban settings, is presented. The Tmrt is derived by modelling shortwave and longwave radiation fluxes in six directions (upward, downward and from the four cardinal points) and angular factors. The model requires a limited number of inputs, such as direct, diffuse and global shortwave radiation, air temperature, relative humidity, urban geometry and geographical information (latitude, longitude and elevation). The model was evaluated using 7 days of integral radiation measurements at two sites with different building geometries – a large square and a small courtyard in Göteborg, Sweden (57°N) – across different seasons and in various weather conditions. The evaluation reveals good agreement between modelled and measured values of Tmrt, with an overall good correspondence of R2 = 0.94, (p {\textless} 0.01, {RMSE} = 4.8 K). {SOLWEIG} 1.0 is still under development. Future work will incorporate a vegetation scheme, as well as an improvement of the estimation of fluxes from the four cardinal points.},
	pages = {697--713},
	number = {7},
	journaltitle = {International Journal of Biometeorology},
	shortjournal = {Int J Biometeorol},
	author = {Lindberg, Fredrik and Holmer, Björn and Thorsson, Sofia},
	urldate = {2017-10-29},
	date = {2008-09-01},
	langid = {english},
	file = {Full Text PDF:/Users/sadaharu/Zotero/storage/3M3UKIC3/Lindberg et al. - 2008 - SOLWEIG 1.0 – Modelling spatial variations of 3D r.pdf:application/pdf;Full Text PDF:/Users/sadaharu/Zotero/storage/V2E9TFUE/Lindberg et al. - 2008 - SOLWEIG 1.0 – Modelling spatial variations of 3D r.pdf:application/pdf;Snapshot:/Users/sadaharu/Zotero/storage/CPDH4C93/Lindberg et al. - 2008 - SOLWEIG 1.0 – Modelling spatial variations of 3D r.html:text/html;Snapshot:/Users/sadaharu/Zotero/storage/TAEJXFI6/s00484-008-0162-7.html:text/html;Snapshot:/Users/sadaharu/Zotero/storage/VAXG4DQF/Lindberg et al. - 2008 - SOLWEIG 1.0 – Modelling spatial variations of 3D r.html:text/html}
